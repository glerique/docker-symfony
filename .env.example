# Copy this file to .env.local and customize for your environment
# 
# Example: cp .env.example .env.local

###> symfony/framework-bundle ###
APP_ENV=dev
APP_SECRET=your-secret-key-here-generate-with-openssl-rand-hex-32
###< symfony/framework-bundle ###

# Database Configuration
DATABASE_URL="postgresql://symfony:secret@db:5432/symfony?serverVersion=16&charset=utf8"

# Docker Configuration
COMPOSE_PROJECT_NAME=docker-symfony

# PHP/Xdebug Configuration (optional - uncomment if needed)
# XDEBUG_MODE=debug
# XDEBUG_START_WITH_REQUEST=yes
# XDEBUG_CLIENT_HOST=host.docker.internal
# XDEBUG_CLIENT_PORT=9003
